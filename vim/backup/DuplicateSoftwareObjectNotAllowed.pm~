package WSB::Exception::DuplicateSoftwareObjectNotAllowed;

use Moose;
extends 'Throwable::Error';
use namespace::autoclean;

has '+message' => (
    is  => 'ro',
    isa => 'Str',
    default => 'Duplicate Software object is not allowed in the container.',
);

__PACKAGE__->meta->make_immutable;

1;
